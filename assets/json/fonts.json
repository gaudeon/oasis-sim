{
    "css": {
        "actual": "assets/css/fonts.css",
        "webpack": "/assets/fonts.css"
    },
    "fonts": {
        "arcade": {
            "font": {
                "actual": "assets/fonts/arcade/ARCADE.TTF",
                "webpack": "/assets/ARCADE.TTF"
            },
            "familyName": "Arcade"
        },
        "arcade-classic": {
            "font": {
                "actual": "assets/fonts/arcade-classic/ARCADECLASSIC.TTF",
                "webpack": "/assets/ARCADECLASSIC.TTF"
            },
            "familyName": "ArcadeClassic"
        },
        "atarian-system": {
            "font": {
                "actual": "assets/fonts/atarian-system/SF Atarian System.ttf",
                "webpack": "/assets/SF Atarian System.ttf"
            },
            "familyName": "SF Atarian System"
        },
        "rubik": {
            "font": {
                "actual": "assets/fonts/rubik/Rubik-Regular.ttf",
                "webpack": "/assets/Rubik-Regular.ttf"
            },
            "familyName": "Rubik"
        }
    },
    "globalTextStyle": {
        "fontSize": 20
    },
    "textStyles": {
        "default": {
            "earth": {
                "font": "rubik",
                "fill": "White",
                "stroke": "White"
            }
        },
        "error": {
            "earth": {
                "font": "rubik",
                "fill": "LightPink",
                "stroke": "LightPink"
            }
        },
        "helpDescription": {
            "earth": {
                "font": "rubik",
                "fill": "Plum",
                "stroke": "Plum"
            }
        },
        "historyTitle": {
            "earth": {
                "font": "rubik",
                "fill": "Plum",
                "stroke": "Plum"
            }
        },
        "historyText": {
            "earth": {
                "font": "rubik",
                "fill": "Orchid",
                "stroke": "Orchid"
            }
        },
        "defaultDescription": {
            "earth": {
                "font": "rubik",
                "fill": "SkyBlue",
                "stroke": "SkyBlue"
            }
        },
        "exitDescription": {
            "earth": {
                "font": "rubik",
                "fill": "LightGreen",
                "stroke": "LightGreen"
            }
        },
        "itemDescription": {
            "earth": {
                "font": "rubik",
                "fill": "LightGreen",
                "stroke": "LightGreen"
            }
        }
    }
}
