{
    "css": {
        "actual": "assets/css/fonts.css",
        "webpack": "/assets/fonts.css"
    },
    "fonts": {
        "arcade": {
            "font": {
                "actual": "assets/fonts/arcade/ARCADE.TTF",
                "webpack": "/assets/ARCADE.TTF"
            },
            "familyName": "Arcade"
        },
        "arcade-classic": {
            "font": {
                "actual": "assets/fonts/arcade-classic/ARCADECLASSIC.TTF",
                "webpack": "/assets/ARCADECLASSIC.TTF"
            },
            "familyName": "ArcadeClassic"
        },
        "atarian-system": {
            "font": {
                "actual": "assets/fonts/atarian-system/SF Atarian System.ttf",
                "webpack": "/assets/SF Atarian System.ttf"
            },
            "familyName": "SF Atarian System"
        },
        "rubik": {
            "font": {
                "actual": "assets/fonts/rubik/Rubik-Regular.ttf",
                "webpack": "/assets/Rubik-Regular.ttf"
            },
            "familyName": "Rubik"
        }
    },
    "defaultTextStyle": {
        "font": "rubik",
        "fontSize": 20,
        "fill": "White",
        "stroke": "White"
    },
    "textStyles": {
        "error": {
            "font": "rubik",
            "fill": "LightPink",
            "stroke": "LightPink"
        },
        "helpDescription": {
            "font": "rubik",
            "fill": "Plum",
            "stroke": "Plum"
        },
        "historyTitle": {
            "font": "rubik",
            "fill": "Plum",
            "stroke": "Plum"
        },
        "historyText": {
            "font": "rubik",
            "fill": "Orchid",
            "stroke": "Orchid"
        },
        "defaultDescription": {
            "font": "rubik",
            "fill": "SkyBlue",
            "stroke": "SkyBlue"
        },
        "exitDescription": {
            "font": "rubik",
            "fill": "LightGreen",
            "stroke": "LightGreen"
        },
        "itemDescription": {
            "font": "rubik",
            "fill": "LightGreen",
            "stroke": "LightGreen"
        },
        "loginText": {
            "font": "arcade",
            "fontSize": 32,
            "fill": "SkyBlue",
            "stroke": "SkyBlue"
        },
        "readyPlayerOneLoginText": {
            "font": "arcade-classic",
            "fontSize": 64,
            "fill": "LightPink",
            "stroke": "LightPink"
        }
    }
}
